{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "windows"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "no-console": [0],
    "no-undefined": [0],
    "env": {
        "es6": true,
        "browser": false
    },
    "extends": "eslint:recommended"
}